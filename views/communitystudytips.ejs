<style>
    /*===== BLOG STYLE ONE =====*/
*{
    margin: 0;
}
body{
    background-image: url("https://cdn.discordapp.com/attachments/931247863491620914/946456193898012693/homebg.png");
    background-size: cover;
    background-position: center; 
}
.blog-title:hover, a:hover{
    cursor: default;
}
.blog-title {
    text-align: center;
}
.blog-style-one {
  margin-top: 50px;
}
.blog-style-one .blog-image {
  overflow: hidden;
  border-radius: 8px 8px 0 0;
  position: relative;
}
.imagediv{
    height: 230px;
    overflow: hidden;
    border-radius: 8px 8px 0 0;
}
.blog-image{
    overflow: hidden;
    height: 230px;
    background-color: red;
    background-position: center;
    background-size: cover;
}
.blog-style-one .blog-image .category {
  background-color: var(--secondary);
  color: var(--white);
  font-size: 13px;
  padding: 7px 20px;
  position: absolute;
  right: 20px;
  top: 20px;
  border-radius: 30px;
  text-decoration: none;
  font-family: "Changa", sans-serif;
  color: #eee;
}
.blog-image{
  transition: all 200ms ease;

}
h {
    text-align: center;
}
.blog-content{
    height: 230px;
    box-shadow: 10px 17px 40px -4px rgba(0,0,0,0.75);

}
.blog-image:hover{
  -webkit-transform: rotate(1deg) scale(1.1);
  -moz-transform: rotate(1deg) scale(1.1);
  -ms-transform: rotate(1deg) scale(1.1);
  -o-transform: rotate(1deg) scale(1.1);
  transform: rotate(1deg) scale(1.1);
}
.blog-style-one .blog-image img {
  width: 100%;
  -webkit-transition: all 0.2s ease-out 0s;
  -moz-transition: all 0.2s ease-out 0s;
  -ms-transition: all 0.2s ease-out 0s;
  -o-transition: all 0.2s ease-out 0s;
  transition: all 0.2s ease-out 0s;
}
.blog-style-one .blog-image:hover img {
  -webkit-transform: rotate(1deg) scale(1.1);
  -moz-transform: rotate(1deg) scale(1.1);
  -ms-transform: rotate(1deg) scale(1.1);
  -o-transform: rotate(1deg) scale(1.1);
  transform: rotate(1deg) scale(1.1);
}
.more, .blog-title>a{
    text-decoration: none!important;
}
.more{
    background-color: var(--primary);
    color: #ddd!important;
    padding: 5px 20px;
    border-radius: 30px;
    -webkit-transition: all 0.2s ease-out 0s;
    -moz-transition: all 0.2s ease-out 0s;
    -ms-transition: all 0.2s ease-out 0s;
    -o-transition: all 0.2s ease-out 0s;
    transition: all 0.2s ease-out 0s;
}
.more:hover{
    background-color: var(--secondary);
    cursor: pointer;
}
.blog-style-one .blog-content {
  padding: 30px;
  border: 1px solid var(--light-1);
  border-radius: 0 0 8px 8px;
  border-top: none;
  background-color: var(--light);
}
@media only screen and (min-width: 768px) and (max-width: 991px) {
  .blog-style-one .blog-content {
    padding: 25px;
  }
}
@media (max-width: 767px) {
  .blog-style-one .blog-content {
    padding: 20px;
  }
}
.blog-style-one .blog-content .blog-title {
  display: block;
  margin-bottom: 10px;
}
.blog-style-one .blog-content .blog-title a {
  font-weight: 600;
  color: var(--primary);
  -webkit-transition: all 0.3s ease-out 0s;
  -moz-transition: all 0.3s ease-out 0s;
  -ms-transition: all 0.3s ease-out 0s;
  -o-transition: all 0.3s ease-out 0s;
  transition: all 0.3s ease-out 0s;
  line-height: 30px;
}
@media (max-width: 767px) {
  .blog-style-one .blog-content .blog-title a {
    line-height: 24px;
  }
}
.blog-style-one .blog-content .blog-title a:hover {
  color: var(--secondary);
}
.blog-style-one .blog-content span {
  font-size: 14px;
  line-height: 20px;
  color: var(--dark-3);
  margin-top: 8px;
  margin-right: 12px;
  display: inline-block;
}
.blog-style-one .blog-content .text {
  color: var(--dark-3);
  margin-top: 16px;
}
.blog-style-one .blog-content .more {
  text-transform: uppercase;
  font-weight: 600;
  color: var(--primary);
  margin-top: 30px;
  display: inline-block;
}
.blog-style-one .blog-content .more:hover {
  color: var(--secondary);
}
:root{
    --primary: #038a5b;
    --secondary: #05cb87;
    --light: #eee;
    --dark: #292929;
}
