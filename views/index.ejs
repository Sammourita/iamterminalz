  <!-- Youtube Video -->

  <section class="video-watching" id="youtube-video-cta">
    <iframe
      src="https://www.youtube.com/embed/4DCx78AY95I"
      title="YouTube video player"
      frameborder="0"
      allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture"
      allowfullscreen
      class="video-watching-video"
    ></iframe>
  </section>

  <!-- Our teacher -->
  <section class="our-teachers">
    <div class="container">
      <h1 class="our-teachers-heading arabic">خذ فكرة عن بعض أساتذتنا</h1>
      <div class="explanations-examples-cards our-teachers-cards">
        <div class="row">
          <% teachers.forEach(function(teacher) { %>
          <div
            class="col-xl-4 col-lg-6 col-md-6 wow fadeInLeft"
            data-wow-delay="0ms"
            data-wow-duration="1000ms"
          >
            <div class="courses-one__single">
              <a href="/teachers/<%= teacher.username %>"
                ><div
                  class="courses-one__single-img"
                  style="height: fit-content; overflow: hidden"
                >
				<% if(teacher.image != 'no image added') { %>
                  <img
                    src="/profiles/<%= teacher.image %>"
                    alt=""
                  />
				  <% } else {  %>
					<img
                    src="https://i.pinimg.com/564x/31/ec/2c/31ec2ce212492e600b8de27f38846ed7.jpg"
                    alt=""
                  />
					<% } %>
                  <div class="overlay-text">
                    <p>Top Teacher</p>
                  </div>
                </div>
              </a>

              <div
                class="courses-one__single-content our-teachers-cards-content"
              >
                <h6 class="courses-one__single-content-name matter"><%= teacher.materials %></h6>
                <h4 class="courses-one__single-content-title">
                  <a href="course-details.html">@<%= teacher.username %></a>
                </h4>
                <h6 class="courses-one__single-content-name experience-years">
                  Experience years: <%= teacher.experience %>
                </h6>
              </div>
            </div>
          </div>
          <% } ) %>
         
